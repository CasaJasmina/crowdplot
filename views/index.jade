extends layout
block content

	- var cols = 13
	- var rows = 19

	div.container


		div.content
			div.col-md-8.col-md-offset-2.col-xs-12

				div#table.col-md-12
					
					h1 CrowdPlot


					table
						thead
						tbody
						-for (var i = 0; i < rows; i++)
							tr
								-for (var j = 0; j < cols; j++)
									td&attributes({'x': j},{'y': i},{'id':j+'_'+i})= subobject
										//div.tableCell&attributes({'x': j},{'y': i})= j+' '+i
										img.tableCell&attributes({'x': j},{'y': i})(src='uploads/png/'+j+'_'+i+'_mini.png', alt='',) 


	
					div#hover
						//div#activeCell
						div#svgWrap.col-md-8.col-md-offset-2.col-xs-12
							svg#svg(version="1.1",xmlns="http://www.w3.org/2000/svg",xmlns:xlink="http://www.w3.org/1999/xlink",width="520px",height="520px")
							
						div.loading
							div.ball
							div.ball1

						a.back.btn back

						a.undo.btn back

						input.btn(
							type='button'
							class="save"
							value="save"
				  		)
		//form(action="save", method="post", enctype="multipart/form-data")
